{
    "env": { "es6": true, "node": true },
    "extends": "eslint:recommended",
    "globals": { "Atomics": "readonly", "SharedArrayBuffer": "readonly" },
    "parserOptions": {
        "sourceType": "script",
        "ecmaVersion": 10,
        "ecmaFeatures": { "globalReturn": false, "impliedStrict": true }
    },
    "rules": {
        "accessor-pairs": "off",
        "require-atomic-updates": "off",
        "semi": "warn",
        "radix": "warn",
        "eqeqeq": "warn",
        "no-new": "warn",
        "no-var": "warn",
        "no-eval": "warn",
        "no-sync": "warn",
        "no-proto": "warn",
        "no-labels": "warn",
        "use-isnan": "warn",
        "no-eq-null": "warn",
        "jsx-quotes": "warn",
        "new-parens": "warn",
        "semi-style": "warn",
        "comma-style": "warn",
        "brace-style": "warn",
        "key-spacing": "warn",
        "no-debugger": "warn",
        "no-new-func": "warn",
        "no-loop-func": "warn",
        "no-dupe-keys": "warn",
        "no-div-regex": "warn",
        "arrow-parens": "warn",
        "comma-dangle": "warn",
        "dot-notation": "warn",
        "guard-for-in": "warn",
        "no-ex-assign": "warn",
        "no-multi-str": "warn",
        "no-obj-calls": "warn",
        "semi-spacing": "warn",
        "valid-typeof": "warn",
        "arrow-spacing": "warn",
        "block-spacing": "warn",
        "comma-spacing": "warn",
        "no-extra-bind": "warn",
        "no-extra-semi": "warn",
        "no-new-object": "warn",
        "no-undef-init": "warn",
        "prefer-spread": "warn",
        "no-lone-blocks": "warn",
        "no-func-assign": "warn",
        "no-new-require": "warn",
        "no-path-concat": "warn",
        "no-self-assign": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "no-else-return": "warn",
        "no-extra-parens": "warn",
        "no-class-assign": "warn",
        "keyword-spacing": "warn",
        "no-implied-eval": "warn",
        "no-new-wrappers": "warn",
        "no-regex-spaces": "warn",
        "no-return-await": "warn",
        "no-self-compare": "warn",
        "no-useless-call": "warn",
        "space-in-parens": "warn",
        "space-infix-ops": "warn",
        "space-unary-ops": "warn",
        "no-multi-spaces": "warn",
        "no-dupe-else-if": "warn",
        "no-throw-literal": "warn",
        "no-sparse-arrays": "warn",
        "no-empty-pattern": "warn",
        "no-control-regex": "warn",
        "no-global-assign": "warn",
        "object-shorthand": "warn",
        "no-setter-return": "warn",
        "no-nested-ternary": "warn",
        "func-call-spacing": "warn",
        "no-duplicate-case": "warn",
        "no-unsafe-finally": "warn",
        "no-useless-concat": "warn",
        "no-useless-escape": "warn",
        "prefer-rest-params": "warn",
        "symbol-description": "warn",
        "yield-star-spacing": "warn",
        "no-floating-decimal": "warn",
        "no-compare-neg-zero": "warn",
        "operator-assignment": "warn",
        "rest-spread-spacing": "warn",
        "space-before-blocks": "warn",
        "no-case-declarations": "warn",
        "no-array-constructor": "warn",
        "max-classes-per-file": "warn",
        "object-curly-newline": "warn",
        "switch-colon-spacing": "warn",
        "no-inner-declarations": "warn",
        "no-dupe-class-members": "warn",
        "no-constant-condition": "warn",
        "prefer-arrow-callback": "warn",
        "no-prototype-builtins": "warn",
        "no-constructor-return": "warn",
        "function-paren-newline": "warn",
        "template-curly-spacing": "warn",
        "no-irregular-whitespace": "warn",
        "no-useless-computed-key": "warn",
        "prefer-numeric-literals": "warn",
        "no-unexpected-multiline": "warn",
        "no-mixed-spaces-and-tabs": "warn",
        "no-empty-character-class": "warn",
        "implicit-arrow-linebreak": "warn",
        "no-async-promise-executor": "warn",
        "computed-property-spacing": "warn",
        "no-shadow-restricted-names": "warn",
        "no-template-curly-in-string": "warn",
        "lines-between-class-members": "warn",
        "prefer-promise-reject-errors": "warn",
        "no-misleading-character-class": "warn",
        "no-whitespace-before-property": "warn",
        "prefer-exponentiation-operator": "warn",
        "curly": ["warn", "all"],
        "quotes": ["warn", "double", { "allowTemplateLiterals": true }],
        "indent": ["warn", 4, { "SwitchCase": 1, "ArrayExpression": "off" }],
        "max-len": ["warn", { "code": 120, "ignoreStrings": false }],
        "no-empty": ["warn", { "allowEmptyCatch": true }],
        "id-length": ["warn", { "exceptions": ["a", "b", "i", "_"] }],
        "func-style": ["warn", "declaration", { "allowArrowFunctions": true }],
        "quote-props": ["warn", "as-needed"],
        "dot-location": ["warn", "property"],
        "getter-return": ["warn", { "allowImplicit": true }],
        "padded-blocks": ["warn", "never"],
        "no-fallthrough": ["warn", { "commentPattern": "break[\\s\\w]*(omitted)?" }],
        "no-cond-assign": ["warn", "always"],
        "no-return-assign": ["warn", "always"],
        "no-empty-function": ["warn", { "allow": ["arrowFunctions", "methods"] }],
        "no-trailing-spaces": ["warn", { "skipBlankLines": true, "ignoreComments": true }],
        "handle-callback-err": ["warn", "^(err|.+Error)$"],
        "max-nested-callbacks": ["warn", 5],
        "no-duplicate-imports": ["warn", { "includeExports": true }],
        "prefer-destructuring": ["warn", { "array": true, "object": false }],
        "array-element-newline": ["warn", "consistent"],
        "array-callback-return": ["warn", { "allowImplicit": true }],
        "array-bracket-newline": ["warn", "consistent"],
        "array-bracket-spacing": ["warn", "never", {
            "singleValue": false,
            "objectsInArrays": false,
            "arraysInArrays": false
        }],
        "generator-star-spacing": ["warn", { "before": false, "after": true }],
        "grouped-accessor-pairs": ["warn", "getBeforeSet"],
        "no-multiple-empty-lines": ["warn", { "max": 4, "maxEOF": 2, "maxBOF": 2 }],
        "padding-line-between-statements": ["warn", {
            "blankLine": "always",
            "prev": "*",
            "next": "return"
        }],
        "no-with": "error",
        "no-caller": "error",
        "no-iterator": "error",
        "no-dupe-args": "error",
        "no-redeclare": "error",
        "no-new-symbol": "error",
        "for-direction": "error",
        "no-delete-var": "error",
        "no-multi-assign": "error",
        "no-const-assign": "error",
        "no-invalid-this": "error",
        "constructor-super": "error",
        "no-this-before-super": "error",
        "no-buffer-constructor": "error",
        "no-undef": ["error", { "typeof": true }],
        "no-invalid-regexp": ["error", { "allowConstructorFlags": ["gi"] }]
    }
}